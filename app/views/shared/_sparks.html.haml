.selected.row.trans
  .fleft.pic= image_tag sparks.selected.details.pic_square
  .fleft.text
    - if current_user.friends_scores_unlocked?
      :erb
        <%="<span class='name'><a href='#{altruist_path(sparks.selected.givey_token)}'>#{sparks.selected.details.name}</a></span> #{sparks.question.phrase} than #{not_selected_names(sparks)}"%>
    - else
      :erb
        <%="<span class='name'>#{sparks.selected.details.name}</span> #{sparks.question.phrase} than #{not_selected_names(sparks)}"%>
