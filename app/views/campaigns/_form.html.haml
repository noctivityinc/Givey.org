= form_for @campaign do |f|
  = f.error_messages
  %p
    = f.label :user_id
    %br
    = f.text_field :user_id
  %p
    = f.label :slots_available
    %br
    = f.text_field :slots_available
  %p
    = f.label :slot_value
    %br
    = f.text_field :slot_value
  %p
    = f.label :video_guid
    %br
    = f.text_field :video_guid
  %p
    = f.label :completed_at
    %br
    = f.text_field :completed_at
  %p
    = f.submit
